{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "jsx": "react-jsx",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["UI/**/*.tsx", "UI/**/*.ts", "global.d.ts"],
  "exclude": ["node_modules", "dist"]
}